{
    "id": "hackaton-realm",
    "realm": "hackaton-realm",
    "accessTokenLifespan": 300,
    "accessTokenLifespanForImplicitFlow": 900,
    "accessTokenLifespanForClientCredentials": 900,
    "roles": [
      {
        "id": "admin",
        "name": "admin",
        "description": "Admin Role"
      },
      {
        "id": "funcionario",
        "name": "funcionario",
        "description": "Funcionario Role"
      }
    ],
    "clients": [
      {
        "clientId": "postech-hackaton-ponto-eletronico",
        "enabled": true,
        "publicClient": true,
        "redirectUris": [
          "http://localhost:8080/*"
        ],
        "bearerOnly": false
      },
      {
        "clientId": "postech-hackaton-funcionarios",
        "enabled": true,
        "publicClient": true,
        "redirectUris": [
          "http://localhost:8081/*"
        ],
        "bearerOnly": false
      },
      {
        "clientId": "postech-hackaton-relatorios",
        "enabled": true,
        "publicClient": true,
        "redirectUris": [
          "http://localhost:8082/*"
        ],
        "bearerOnly": false
      }
    ]
  }
  